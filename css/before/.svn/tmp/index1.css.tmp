		
			#footer {
				width: 100%;
				background: #444;
				padding: 40px 0 20px;
				color: #CCCCCC;
			}
			
			#footer .container {
				margin: auto;
				min-width: 992px;
			}
			
			.table {
				width: 100%;
				text-align: left;
				line-height: 1.5em;
				font-size: 1strong;
				text-indent: 20px;
			}
			
			.table tbody tr th {
				font-size: 16px;
				border-top: 0;
			}
			
			.table tbody tr td {
				font-size: 12px;
				border-top: 0;
			}
			
			.footer-info {
				text-align: center;
				font-size: 12px;
				margin-left: -10px;
				padding: 20px 0 15px;
			}
			
			.footer-info p a {
				color: #CCCCCC;
				margin-left: 10px;
				text-decoration: none;
			}
			
			.footer-info p strong {
				margin-left: 5px;
			}
			
			.footer-info p a:hover {
				color: red;
			}
			/*楼层单元*/
				
			.floors{padding: 10px 0 0 0;margin: 0;border-bottom: 1px solid #D22B2B;}
			.floorName{display: inline-block;font-size: 2rem;position: relative;}
			.floorName .floorNum{position: absolute;left: 0px;top: 5px;font-size: 1.8rem;color: white;}
			.floorName .className{margin-left: 15px;color: #666;}
			.fClasses{display: inline-block;float: right;}
			.fClasses span{font-size: 1.3rem;padding-top: 1%;border: 1px solid lightgray;display: inline-block; width: 100px;height: 35px;text-align: center;line-height: 30px;border-bottom: 0;margin-bottom: -1px;}
			.container.items {
				min-width: 992px;
				padding: 0;
/*				border: 1px solid gray;*/
			}
			#floorcontent{
				height: 494px;
				overflow: hidden;
			}
			.bigitem {
				height: 436px;
				padding: 0;
				display: inline-block;
				overflow: hidden;
				position: relative;
			}
			.bigitem_one{
				width: 100%;
				height: 75%;
			}
			.bigitem_one img{
				width: 100%;
				height: 100%;
			}
			.bigitem .swiper-container{
				width: 100%;
				height: 5%;
				position: absolute;
				top: 70%;
				background: rgba(180,180,180,0.6);
			}
			.bigitem .swiper-container .swiper-slide{
				text-align: center;
				font-weight: bold;
			}
			.bigitem_two{
				width: 100%;
				height: 25%;
				background: #33BE40;
				text-align: center;
				padding-top: 5%;
			}
			.bigitem_two span{
				display: block;
				color: white;
			}
			.bigitem_two span:nth-child(1){
				font-size: 1.9em;
			}
			.bigitem_two span:nth-child(2){
				font-size: 1.2em;
			}
			.bigitem img {
				width: 100%;
			}
			
			.bigitem .row {
				margin: 0;
			}
			
			.bigitem .row span {
				background: rgba(0, 0, 0, 0.3);
				color: #444;
				font-weight: 900;
				display: inline-block;
				padding: 2%;
				border-bottom: 1px solid lightgray;
				text-align: center;
			}
			
			.bigitem .row span:nth-child(1) {
				border-right: 1px solid lightgray;
			}
			
			.bigitem .row span:nth-child(3) {
				border-right: 1px solid lightgray;
			}
			.shopitemBox{padding: 0;}
			.shopitem {
				padding: 0;
				border-right: 1px solid lightgray;
				border-bottom: 1px solid lightgray;
				display: inline-block;
				height: 218px;
			}
			
			.shopitem img {
				width: 60%;
				margin-left: 15%;
			}
			.goodname{
				margin-bottom: 0;
			}
			.goodname,
			.goodprice {
				padding-left: 10px;
				font-size: 1.4rem;
			}
			
			.goodprice {
				color: red;
			}
			.floorClass:nth-child(1){
				border:1px solid #D22B2B;
				border-bottom:1px solid white;
			}

/*店铺街*/
.store {
	margin-top: 120px;
				min-width: 992px;
				padding: 0;
				border-top: 1px solid lightgray;
				
			}
.storeImg{padding: 0;width: 25%;height: 305px;overflow: hidden;}
.storeImg img{width: 100%;}
.storelogo{border-right:1px solid lightgray;border-bottom: 1px solid lightgray;height: 76.5px;}
.storelogo img{width: 90%;margin-top: 10%;margin-left: 7%;}
#storeName{width: 100%;background: rgba(0,0,0,0.3);}
